<!DOCTYPE html>
<html>

<head>
    <meta charset="UTF-8">

    <h1>{% block title %}Chat{% endblock %}</h1>

    {% block stylesheets %}
    {{ encore_entry_link_tags('app') }}
    <link rel="stylesheet" href="{{ asset('css/styles.css') }}">
    
    {#BOOSTRAP#}
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL"
        crossorigin="anonymous"></script>


    {#JQUERY#}
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    

    {#FUENTE#}
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link
        href="https://fonts.googleapis.com/css2?family=Oswald:wght@300;400&family=Roboto:ital,wght@1,300&family=Rubik:wght@300&display=swap"
        rel="stylesheet">
    {% endblock %}
</head>


<body>
    {% block body %}
    {% endblock %}
</body>

<footer>
    <div class="container-footer w-container">
        <div class="w-row">
            <div class="footer-column w-clearfix w-col w-col-4">
                <img src="{{ asset('') }}" alt="" width="40" class="failory-logo-image">
                <h3 class="footer-failory-name">Chat</h3>
                <p class="footer-description-failory">© 2023 Chat, inc.</p>
            </div>
            <div class="footer-column w-col w-col-8">
                <div class="w-row">
                    <div class="column-center-mobile w-col w-col-12">
                        <div style="display: flex;">
                            <h3 class="footer-titles" style="margin-right: 90px;">Sígueme en mis redes ! >></h3>
                            <div>
                                <h3 class="footer-titles">Valentino</h3>
                                <a href="https://www.linkedin.com/in/valentino-borgo-a185b2258/" target="_blank" class="footer-social-network-icons w-inline-block"><img width="40" height="40" src="https://img.icons8.com/ios-filled/50/linkedin.png" alt="linkedin" style="background-color: white; border-radius: 0.5rem;"/></a>
                                <a href="https://github.com/ValentinoBorgo" target="_blank" class="footer-social-network-icons w-inline-block"><img width="40" height="40" src="https://img.icons8.com/ios-filled/50/github.png" alt="github" style="background-color: white; border-radius: 0.5rem;"/></a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</footer>

</html>